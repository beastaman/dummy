<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>WelcomeController</servlet-name>
  	<display-name>WelcomeController</display-name>
  	<description></description>
  	<servlet-class>com.controller.WelcomeController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Controller</servlet-name>
  	<display-name>Controller</display-name>
  	<description></description>
  	<servlet-class>com.controller.Controller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BranchController</servlet-name>
  	<display-name>BranchController</display-name>
  	<description></description>
  	<servlet-class>com.controller.BranchController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServiceController</servlet-name>
  	<display-name>ServiceController</display-name>
  	<description></description>
  	<servlet-class>com.controller.ServiceController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomerController</servlet-name>
  	<display-name>CustomerController</display-name>
  	<description></description>
  	<servlet-class>com.controller.CustomerController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>WelcomeController</servlet-name>
  	<url-pattern>/WelcomeController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Controller</servlet-name>
  	<url-pattern>/Controller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BranchController</servlet-name>
  	<url-pattern>/BranchController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServiceController</servlet-name>
  	<url-pattern>/ServiceController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomerController</servlet-name>
  	<url-pattern>/CustomerController</url-pattern>
  </servlet-mapping>
</web-app>
