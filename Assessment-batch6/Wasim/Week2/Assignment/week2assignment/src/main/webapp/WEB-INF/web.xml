<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>BranchController</servlet-name>
  	<display-name>BranchController</display-name>
  	<description></description>
  	<servlet-class>com.sapient.controller.BranchController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ItemController</servlet-name>
  	<display-name>ItemController</display-name>
  	<description></description>
  	<servlet-class>com.sapient.controller.ItemController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>OrderController</servlet-name>
  	<display-name>OrderController</display-name>
  	<description></description>
  	<servlet-class>com.sapient.controller.OrderController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ConfirmationController</servlet-name>
  	<display-name>ConfirmationController</display-name>
  	<description></description>
  	<servlet-class>com.sapient.controller.ConfirmationController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>BranchController</servlet-name>
  	<url-pattern>/BranchController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ItemController</servlet-name>
  	<url-pattern>/ItemController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>OrderController</servlet-name>
  	<url-pattern>/OrderController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ConfirmationController</servlet-name>
  	<url-pattern>/ConfirmationController</url-pattern>
  </servlet-mapping>
  
</web-app>
